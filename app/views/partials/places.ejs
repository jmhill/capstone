<!-- Form to submit place data -->
<form action="/places" method="post" role="form">
    <div class="form-group">
      <label>Country:</label>
      <input type="text" class="form-control" name="country" />
    </div>
    <div class="form-group">
      <label>City:</label>
      <input type="text" class="form-control" name="city" />
    </div>
    <div class="form-group">
      <input type="submit" class="btn btn-default" value="Add a new place" />
    </div>
	</form>

<ul>
<% places.forEach(function(place) { %>
	<li> <%= place.city %>, <%= place.country %> </li>
<% }); %>
</ul>